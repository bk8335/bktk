<!DOCTYPE html>
<html>
  <head>
    <title>BKTK - <%= yield(:title) %></title>
    <%= render '/google_analytics' %>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
